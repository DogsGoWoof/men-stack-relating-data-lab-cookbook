<%- include('../partials/_head-style.ejs') %>
        <title>My Pantry</title>
    </head>

    <%- include('../partials/_cupboard-open-close.ejs') %>

        <%- include('../partials/_navbar.ejs') %>
        <h1><ruby> 食料品 <rp>(</rp><rt>Foodstuff</rt><rp>)</rp> </ruby></h1>
        <div class="button">
            <a href="/users/<%= user._id %>/foods/new"><span class="emoji-icon">🗒️</span>+<ruby> 品 <rp>(</rp><rt>Stuff</rt><rp>)</rp> </ruby></a>
        </div>
        <ul>
        <% foods.forEach((food) => { %>
            <div class="food-card">
                <a href="/users/<%= user._id %>/foods/<%= food._id %>">
                    <li>
                        <%= food.name %>
                    </li>
                </a>
            </div>
        <% }); %>
        </ul>

    </body>

</html>